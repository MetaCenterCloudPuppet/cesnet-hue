<% if @environment -%>
<% @environment.keys.each do |k| -%>
<% if @environment[k] == '::undef' -%>
rm /files<%= @defaultdir -%>/hue/<%= k %>
<% else -%>
set /files<%= @defaultdir -%>/hue/<%= k -%> <%= @environment[k] %>
set /files<%= @defaultdir -%>/hue/<%= k -%>/export ""
<% end -%>
<% end -%>
<% end -%>
